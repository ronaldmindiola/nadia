{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["node_modules", "dist", "public", "vite.config.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["src/components/*"],
      "@ui/*": [
        "./src/components/ui/*"
      ],
      "@content/*": ["src/content/*"],
      "@data/*": ["src/data/*"],
      "@images/*": ["src/images/*"],
      "@scripts/*": ["src/assets/scripts/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@utils/*": ["src/utils/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@types/*": ["src/types/*"],
      "@logos/*": ["src/images/logos/*"]
    },
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
